{
  "extends": "airbnb",
  "globals": {
    "angular": true,
    "describe": true,
    "it": true,
    "beforeEach": true
  },
  "rules": {
    "indent": [2, 2, {
      "SwitchCase": 1,
      "VariableDeclarator": {
        "var": 2,
        "let": 2,
        "const": 3
      }
    }],
    "func-names": 0,
    "no-param-reassign": 0,
    "one-var": 0,
    "prefer-arrow-callback": 0
  }
}
